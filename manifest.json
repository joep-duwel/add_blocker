{
  "name": "Joep's Google Chrome Adblocker",
  "version": "1.0",
  "description": "A simple Google Chrome adblocker",
  "manifest_version": 3,
  "permissions": [
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  }
}
